
<div class="container-fluid  bg-black barraOne" id="barraNav" > 
{{>barraHeadIdem}}
<div class="container-fluid  bg-black barraPrint" id="barraNavTop" > 
{{>barraHeadIdem}}
<script>
    setInterval(transicao, 250);

    let barra = document.querySelector("#barraNavTop")

    function transicao(){
        let top = window.scrollY
        console.log(top)
        if(top >= 50){

            barra.classList.remove("barraNotPrint")
            barra.classList.add("barraPrint")
        }
        else{
            barra.classList.remove("barraPrint")
            barra.classList.add("barraNotPrint")
            
        } 
        
    }
</script>