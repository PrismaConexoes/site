<div class="d-flex flex-column justify-content-start align-items-start m-5">
    {{#each data}}
        <div class="d-flex flex-row border border-1 justify-content-center align-items-center">
            <div class="col d-dlex justify-content-center align-items-center m-2">
                {{this.email}}
            </div>
            <div class="col">
                <script>
                    (data) => {
                        if(data.email != logado){
                            return <a href="/delAdm/{{this.id}}" ><input type="button"  class="btn btn-secondary m-2" style="width: 150px;" value="Apagar" /></a>
                        }
                    }
                </script>     
            </div>
        </div>
    {{/each}}
    <div class="d-flex justify-content-start align-items-start">
        <a href="/administrarSite" ><input type="button"  class="btn btn-secondary m-2" style="width: 150px;" value="Voltar" /></a>
    </div>
    
</div>